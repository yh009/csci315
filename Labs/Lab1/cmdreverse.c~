#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main(int argc,char *argv[])
{
    if (argc != 2){
        printf("Err");
    }
    else{
        char *aStr = argv[1];
        strrev(aStr);
        printf("Reversed str: %s", aStr);
    }
    return 0;
    }
