CC = gcc
CFLAGS = -Wall -lpthread
EXECS = dp problem1 problem2 problem3-1

all: $(EXECS)

dp:dp.c
	$(CC) $(CFLAGS) dp.c -o dp

problem1:problem1.c
	$(CC) $(CFLAGS) problem1.c -o problem1

problem2:problem2.c
	$(CC) $(CFLAGS) problem2.c -o problem2

problem3-1:problem3-1.c
	$(CC) $(CFLAGS) problem3-1.c -o problem3-1

.PHONY: clean
clean:
	rm -rf $(EXEC) *.o
